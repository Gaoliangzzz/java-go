public class test1{
	public static void main(String[] args) {
//		int nums[] = {1,2,3,4,5};
//		print1(nums);
		print2(1,2,3,4,5);
	}
	public static void print1(int x[]) {
		int len = x.length;
		for(int i=0;i<len;i++) {
			System.out.println(x[i]);
		}
	}
	public static void print2(int...x) {//注意：可变参数的写法，int...x，函数的最后一个参数才可以用可变参数
//		int len = x.length;
		for(int i:x) {
			System.out.println(x[i-1]);//注意：区分数组下标和数组元素
//			System.out.println(x.length);
		}
	}
}
